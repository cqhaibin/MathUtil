<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;netcoreapp3.1;net5.0</TargetFrameworks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Product>MathUtil</Product>
    <PackageId>MathUtil</PackageId>
    <Version>1.0.0</Version>
    <Authors>itachijames</Authors>
    <Company>itachijames</Company>
    <Description>数学工具库</Description>
    <Copyright>© ITACHIJAMES. All Rights Reserved.</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/itachijames/MathUtil.git</PackageProjectUrl>
    <RepositoryUrl>https://github.com/itachijames/MathUtil.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Math,Geometry,Algebra</PackageTags>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>

</Project>
